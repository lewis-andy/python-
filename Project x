command = ''
while command.lower() != "quit":
    command = input('> ').lower()
    if command == "start":
        print('The car started......')
    elif command == "stop":
        print('the car stopped.')
    elif command == "help":
        print('start - to start the car')
        print('stop - to stop the car')
        print('quit to exit the game')
    elif command == "quit":
        print('Exit the game')
    else:
        print('invalid command')
